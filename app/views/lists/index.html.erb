<h1>My lists:</h1>
<% @lists.each do |list| %>
  <%= link_to(list.name, list_path(list))  %>
<% end %>

</div>
<div class="container">
  <h2 class="text-center">Create a new list</h2>
  <div class="row">
    <div class="col-4 offset-4">
    <%= simple_form_for(@list) do |f|%>
    <%= f.input :name %>

    <%= f.submit %>
    <% end %>
    </div>
  </div>
</div>
